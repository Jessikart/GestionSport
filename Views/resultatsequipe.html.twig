{% extends "index.html.twig" %}

{% block section %}
<h2 class="text-info bg-dark">Résultats de la recherche pour les équipes</h2>
<table class="table table-hover table-condensed">
    <thead>
		<tr>
            <th>Nom de l'équipe</th>
            <th>Nombre de victoires</th>
        </tr>
	</thead>
    <tbody>
	
    {% for equipes in resultats %}
	<tr>
        <td>{{ equipes.nom }}</td>
        <td>{{ equipes.victoires }} </td> 
	</tr>
    {% endfor %}
	</tbody>
</table>

{% endblock %}
